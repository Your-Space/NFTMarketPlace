@model IEnumerable<NftViewItemModel>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<br /><br />
<div class="row">
    <div class="col-6">
        <h2 class="text-dark">Nft for sale</h2>
    </div>
    <div class="col-6 text-right ">
        <a asp-action="Create" class="btn btn-dark"><i class="fas fa-plus"></i>&nbsp;Add New Nft</a>
    </div>

</div>
<br />
<br/><br/>
<div class="row">
    @foreach (var item in Model)
    {
        @Html.Partial("_NftPartial", item)
    }
</div>